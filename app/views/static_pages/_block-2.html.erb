<div class="block-2">
  <div class="container">
    <div class="text-center text-block-2">
      <div>
        <h2 class="dark-back m-0"><%= t ".title" %></h2>
      </div>
      <div class="mt-3">
        <p><%= t ".text-1" %></p>
        <p><%= t ".text-2" %></p>
      </div>
    </div>
    <div class="row">
      <div class="col-md-4 mb-4">
        <div class="card shadow-0">
          <%= link_to "#" do %>
            <%= image_tag Settings.image_block_2, class: "card-img-top" %>
          <% end %>
          <div class="card-body">
            <h5 class="card-title">
              <%= link_to t(".philip_dating"), "#" %>
            </h5>
            <p class="card-text">
              <%= t ".summary" %>
            </p>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
